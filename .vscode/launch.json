  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run app",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "C:\\ProgramData\\chocolatey\\bin\\deno.EXE",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "-A",
        "--allow-read",
        "--config",
        "config.json",
        "${workspaceFolder}/src/main.ts"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Run test",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "C:\\ProgramData\\chocolatey\\bin\\deno.EXE",
      "runtimeArgs": [
        "test",
        "--inspect-brk",
        "-A",
        "--allow-read",
        "--config",
        "config.json",
        "${file}"
      ],
      "attachSimplePort": 9229
    }
  ]
}
